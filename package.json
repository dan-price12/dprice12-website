{
    "name": "stc-website",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "cypress:open": "turbo run cypress:open",
        "cypress:run": "turbo run cypress:run",
        "storybook": "turbo run storybook --parallel"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "eslint-config-custom": "workspace:*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "pnpm@8.11.0"
}
